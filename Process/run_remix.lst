5251   
5252  * // ## Load links from gdx file
5254   
5255   
5256  set transfer_hasflowProfileIn(linksData,years,transfer_techs,pc_transfer_flowProfile);
5257  option transfer_hasflowProfileIn < transfer_flowProfileIn;
