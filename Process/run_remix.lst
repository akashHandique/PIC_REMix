5280      = sum(timeData$timeMapping(timeData,timeModelToCalc),
5281            transfer_flowProfileIn(timeData,linksData,yearsToCalc,transfer_techs,pc_transfer_flowProfile)
5282            / timeLength(timeModelToCalc));
