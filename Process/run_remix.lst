2848  converter_useRampPos(nodesModelToCalc,yearsToCalc,converter_techs,vintage)
2849    $(sum(indicator$accounting_converterStartup(indicator,nodesModelToCalc,yearsToCalc,converter_techs,vintage,"perRamp"), 1)
