4458      =e=
4459      sum ((years_a)
4460                      $(storage_availTech(nodesModelSel,years_a,storage_techs,vintage)
4461                          and years_a.val < sum(yearsToCalc_a$(ord(yearsToCalc_a)=1), yearsToCalc_a.val)
