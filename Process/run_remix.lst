4207                          and yearsToCalc.val + storage_techParam(storage_techs,vintage,'LifeTime') > yearsSel.val
4208                          and yearsToCalc.val <= yearsSel.val ),
4209              storage_unitsBuild(nodesModelSel,yearsToCalc,storage_techs,vintage));
