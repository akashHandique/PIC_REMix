3551      ..
3552      sum(years$
3553            ((converter_decomTech(nodesModelSel,years,converter_techs,vintage))
3554              and years.val < sum(yearsToCalc_a$(ord(yearsToCalc_a)=1), yearsToCalc_a.val)),
