4435                          and years_a.val <= yearsSel.val ),
4436              storage_unitsBuild(nodesModelSel,years_a,storage_techs,vintage))
4437   
4438      + sum ((yearsToCalc)
4439                      $(storage_availTech(nodesModelSel,yearsToCalc,storage_techs,vintage)
