5364              or transfer_usedTech(linksModelToCalc,yearsToCalc,transfer_techs,vintage))
5365              and yearsToCalc.val >= sum(yearsToCalc_a$(ord(yearsToCalc_a)=1), yearsToCalc_a.val)
5366              and yearsToCalc.val <= yearsSel.val),
