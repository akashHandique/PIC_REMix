4912  * // ## Load links from gdx file
4914   
4915   
4916  set transfer_hasflowProfileIn(linksData,years,transfer_techs,pc_transfer_flowProfile);
4917  option transfer_hasflowProfileIn < transfer_flowProfileIn;
